{
  "name": "@opencall/client",
  "version": "0.1.0",
  "description": "React PWA client for OpenCall",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:workers && npm run build:wasm && tsc && vite build",
    "build:wasm": "cd ../protocol/wasm && ./build.sh",
    "build:analyze": "ANALYZE=true npm run build",
    "build:cdn": "VITE_USE_CDN=true npm run build",
    "build:workers": "webpack --config webpack.worker.config.js",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist node_modules .turbo public/workers"
  },
  "dependencies": {
    "@opencall/core": "workspace:*",
    "@tanstack/react-query": "^5.13.4",
    "@vite/vitejs": "^2.3.19",
    "@vite/vitejs-ws": "^2.3.19",
    "dompurify": "^3.0.8",
    "events": "^3.3.0",
    "ipfs-http-client": "^60.0.1",
    "marked": "^11.1.1",
    "mediasoup-client": "^3.7.3",
    "qrcode": "^1.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "simple-peer": "^9.11.1",
    "workbox-window": "^7.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.23.3",
    "@babel/plugin-transform-react-inline-elements": "^7.23.3",
    "@playwright/test": "^1.40.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/dompurify": "^3.0.5",
    "@types/events": "^3.0.3",
    "@types/marked": "^6.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/simple-peer": "^9.11.8",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.26.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.0",
    "wasm-opt": "^1.4.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}